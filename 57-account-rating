public void setAccountRating(Account a) {
    //code here
    if(a==null || a.AnnualRevenue==null){
        return;
    }
    else if(a.AnnualRevenue<=100000){
        a.Rating='Cold';
    }
    else if(a.AnnualRevenue>=100000 and a.AnnualRevenue<=500000){
        a.Rating='Warm';
    }
    else if(a.AnnualRevenue>500000){
        a.Rating='Hot'
    }
    Database.Update(a);
}