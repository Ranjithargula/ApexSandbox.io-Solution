public Id classFromCourse(String courseName) {
    // code here
    apxio__Course__c course = [select id,Name from apxio__Course__c where Name=:courseName limit 1];
    if(course!=null){
        apxio__Class__c cls = new apxio__Class__c();
        cls.Name=course.Name;
        cls.apxio__Course__c =course.Id;
        Database.Insert(cls);
        return cls.Id;
    }
    else{
        return null;
    }
}